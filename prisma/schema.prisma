generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["clientExtensions"]
  
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  firstName String
  lastName  String
  email     String   @unique
  password  String
  ssn       String?
  dob       String?
  address   String?
  plan      String?
  createdAt DateTime @default(now())
}
